{
  "name": "purge-css-webpack",
  "description": "Include Bootstrap's source Sass and minimize it, using purge-css.",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack ./src/index.js --progress",
    "prod": "NODE_ENV=production webpack ./src/index.js --progress"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "5.2.0-beta1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
